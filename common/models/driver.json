{
  "name": "driver",
  "plural": "drivers",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "avatar": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "region": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "job": {
      "type": "hasMany",
      "model": "job",
      "foreignKey": "driverId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": true
      }
    },
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
